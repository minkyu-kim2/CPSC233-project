<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="473.0" prefWidth="355.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.CarPageController">
   <children>
      <GridPane>
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="173.0" minWidth="10.0" prefWidth="84.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="271.0" minWidth="10.0" prefWidth="271.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label fx:id="nameLabel" text="" GridPane.columnIndex="1" />
            <Label text="name: " />
            <Label text="model: " GridPane.rowIndex="1" />
            <Label fx:id="modelLabel" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label text="make:" GridPane.rowIndex="2" />
            <Label fx:id="makeLabel" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Label text="year:" GridPane.rowIndex="3" />
            <Label fx:id="yearLabel" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Label text="Quantity: " GridPane.rowIndex="4" />
            <Label fx:id="quantityLabel" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <Label text="Category:" GridPane.rowIndex="5" />
            <Label fx:id="categoryLabel" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <Label text="Unit price: " GridPane.rowIndex="6" />
            <Label fx:id="priceLabel" GridPane.columnIndex="1" GridPane.rowIndex="6" />
         </children>
      </GridPane>
      <VBox prefHeight="129.0" prefWidth="355.0">
         <children>
            <Label text="Detail:">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin></Label>
            <Label fx:id="detailLabel" />
         </children>
      </VBox>
      <HBox prefHeight="0.0" prefWidth="355.0">
         <children>
            <Button mnemonicParsing="false" onAction="#delete" text="Delete" />
            <Button mnemonicParsing="false" onAction="#edit" text="Edit" />
            <Button mnemonicParsing="false" onAction="#close" text="Close" />
         </children>
      </HBox>
   </children>
</VBox>
